
*test-nestest: nestest.expected nestest.actual
  git diff --word-diff=color $ins # | head -20

nestest.size: nestest.out
  echo $$(cat $< | wc -l) > $out

nestest.expected: nestest.golden nestest.size
  cat $in1 | head -$$(cat $in2) > $out

nestest.actual: nestest.out nestest.size
  cat $in | head -$$(cat $in2) > $out

nestest.out : ../src/nesh.exe nestest.nes
  ./$in $in2 --no-sdl --init-pc 0xC000 --trace-cpu --stop-at 14575 > $out || true
